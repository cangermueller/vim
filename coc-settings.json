{
  "languageserver": {
    "ciderlsp": {
      "command": "/google/bin/releases/cider/ciderlsp/ciderlsp",
      "args": [
        "--tooltag=coc-nvim",
        "--noforward_sync_responses"
      ],
      "filetypes": [
        "borg",
        "c",
        "cpp",
        "go",
        "java",
        "kotlin",
        "proto",
        "python",
        "textproto"
      ]
    }
  },
  "diagnostic.virtualText": false,
  "diagnostic.checkCurrentLine": true,
  "suggest.maxCompleteItemCount": 20,
  "inlayHint.enable": false
}
